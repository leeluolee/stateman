// by default keyframes can auto prefixr
// base
@-o-keyframes shake {
    from {
        transform: translate(0,0);
    }
    10%, 20% {
        transform: translate(0,0);
    }
    20% {
        transform: translate(0,0);
    }
    to {
        transfrom: translate(0,0); 
    }
}


// keyframes in name
$outportShake = (){
    // base
    @keyframes name{
        from {
            transform: scaleX(0);
        }
        10%,100% {
            transform: scaleX(0);
        }
        20% {
            transform: scaleX(0);
        }
        to {
            transfrom: scaleX(0); 
        }
    }
}


@keyframes hello{
        from {
            transform: scaleX(0);
        }
        10% {
            transform: scaleX(0);
        }
        20% {
            transform: scaleX(0);
        }
        to {
            transfrom: scaleX(0); 
        }
}

$outportShake();

